{
  "name": "scenemover-visual-studio-code-extension",
  "displayName": "scenemover - Move fountain scenes around",
  "description": "",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.23.0"
  },
  "icon": "public/icon.png",
  "categories": [
    "Extension Packs",
    "Visualization"
  ],
  "keywords": [
    "ramda",
    "functional programming",
    "fp",
    "interactive",
    "guide"
  ],
  "publisher": "iskenxan",
  "activationEvents": [
    "onCommand:scenemover.start"
  ],
  "main": "./build/ext-src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "scenemover.start",
        "title": "Open interactive scene layout editor",
        "category": "Scene Mover"
      }
    ]
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.7.2",
    "array-move": "^4.0.0",
    "react": "^17.0.1",
    "react-dnd": "^15.1.1",
    "react-dnd-html5-backend": "^15.1.2",
    "react-dom": "^17.0.1",
    "react-markdown": "^5.0.3",
    "react-redux": "^7.2.6",
    "redux": "^4.1.2",
    "terser": "3.14.1"
  },
  "resolutions": {
    "terser": "3.14.1"
  },
  "scripts": {
    "vscode:prepublish": "./scripts/build-non-split.js && tsc -p tsconfig.extension.json",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "start": "react-scripts start",
    "build": "./scripts/build-non-split.js && tsc -p tsconfig.extension.json",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "watch-build": "nodemon --watch src --ext js,tsx,ts --exec \"npm run build\"",
    "lint": "./node_modules/eslint/bin/eslint.js 'src/**/*.ts'",
    "prettify": "prettier '**/*.ts' --write",
    "lintFull": "npm run prettify && npm run lint -- --fix"
  },
  "devDependencies": {
    "@types/jest": "^23.3.13",
    "@types/node": "^17.0.18",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "eslint-config-rickschubert": "^1.0.11",
    "eslint-plugin-import": "^2.25.4",
    "nodemon": "^2.0.7",
    "prettier": "^2.5.1",
    "react-scripts": "^2.1.3",
    "rewire": "^4.0.1",
    "typescript": "^4.2.2",
    "vscode": "^1.1.37"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/iskenxan/sheepy-fp-guide.git"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
